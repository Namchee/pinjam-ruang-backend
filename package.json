    "@types/bcrypt": "^10.0.28-alpha",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/eslint": "^4.16.6",
    "@types/express": "^4.17.0",
    "@types/hapi__joi": "^15.0.2",
